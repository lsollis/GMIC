{
  "format_version": 2,
  "executors": [
    {
      "tasks": ["get_weights", "train", "validate", "test", "submit_model"],
      "executor": {
        "id": "GMICFederatedExecutor",
        "path": "nvflare_code.bc_executor.GMICFederatedExecutor",
        "args": {
          "epochs": 2,
          "lr": 0.0001,
          "batch_size": 4,

          "data_path": "/workspace/data/gmic_format_xai.csv",
          "image_path": "/workspace/data/XAI_output",
          "input_format": "csv",
          "enable_preprocessing": true,
          "force_preprocessing": true,
          "cache_validation": true,
          "use_predefined_splits": true,
          "val_split": 0.2,
          "test_split": 0.2,
          "random_seed": 42,
          "output_dir": "/workspace/data/processed",
          "num_processes": 4,

          "model_path": "/workspace/models/sample_model_1.p",

          "gmic_parameters": {
            "gpu_number": 0,
            "max_crop_noise": [100, 100],
            "max_crop_size_noise": 100,
            "cam_size": [46, 30],
            "K": 6,
            "crop_shape": [256, 256],
            "post_processing_dim": 256,
            "num_classes": 2,
            "use_v1_global": false,
            "percent_t": "auto:1"
          },

          "loss": "bce_with_logits",
          "optimizer": { "name": "adam", "lr": 0.0001, "weight_decay": 0.0 },

          "task_names": {
            "pre_train": "get_weights",
            "train": "train",
            "validate": "validate",
            "test": "test",
            "submit_model": "submit_model"
          }
        }
      }
    }
  ],
  "task_data_filters": [],
  "task_result_filters": [],
  "components": []
}
